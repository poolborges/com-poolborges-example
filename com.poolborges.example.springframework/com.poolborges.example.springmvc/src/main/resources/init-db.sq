DROP TABLE USERS IF EXISTS CASCADE;
CREATE TABLE USERS (
  ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY, 
  USERNAME VARCHAR(255),
  FIRSTNAME VARCHAR(255),
  LASTNAME VARCHAR(255));
  
